        <externalId>false</externalId>
        <formula>if( 
     AND(NOT(ISBLANK(PROJECT_MANAGER__c)), 
               OR(LIQUIDATED_DAMAGES__c = True,
                  PROJ_MGMT_LETTER_OF_CREDIT__c = True,
                  PROJ_MGMT_LIEN_WAIVERS__c = True,
                  PROJ_MGMT_PERFORMANCE_BOND__c = True,
                  PROJ_MGMT_SUPPLY_BOND__c = TRUE,
                  Total_Value__c &gt; 1000000)
         ),
     true,
     false
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
